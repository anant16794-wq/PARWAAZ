  }

  header {
    background: #2c3e50;
    color: #fff;
    padding: 12px 20px;
    display: flex;
    justify-content: space-between;
    align-items: center;
  }
  header h1 {
    margin: 0;
    font-size: 20px;
  }
  .header-buttons {
    display: flex;
    gap: 10px;
  }
  .header-buttons button {
    background: #3498db;
    border: none;
    color: #fff;
    padding: 8px 14px;
    border-radius: 4px;
    cursor: pointer;
    font-size: 14px;
    transition: 0.3s;
  }
  .header-buttons button:hover {
    background: #2980b9;
  }

  .search-bar {
    margin: 20px;
    text-align: right;
  }
  .search-bar input {
    padding: 10px;
    width: 250px;
    border: 1px solid #ccc;
    border-radius: 25px;
    outline: none;
    transition: 0.3s;
  }
  .search-bar input:focus {
    border-color: #3498db;
    box-shadow: 0 0 5px rgba(52, 152, 219, 0.5);
  }

  table {
    width: 95%;
    margin: 20px auto;
    border-collapse: separate;
    border-spacing: 0;
    box-shadow: 0 8px 16px rgba(0,0,0,0.1);
    border-radius: 10px;
    overflow: hidden;
    background: #fff;
  }

  th, td {
    padding: 14px 20px;
    text-align: left;
    font-size: 14px;
  }

  th {
    background: linear-gradient(to right, #3498db, #2980b9);
    color: #fff;
    font-weight: 600;
    letter-spacing: 0.5px;
  }

  tr {
    transition: background 0.3s, transform 0.2s;
  }

  tr:nth-child(even) {
    background: #f9f9f9;
  }

  tr:hover {
    background: #eaf3fc;
    transform: scale(1.01);
  }

  td.institution {
    display: flex;
    align-items: center;
    gap: 10px;
  }

  .view-btn {
    display: none;
    background: #e67e22;
    border: none;
    color: #fff;
    padding: 6px 12px;
    border-radius: 20px;
    cursor: pointer;
    font-size: 13px;
    transition: 0.3s;
  }
  .view-btn:hover {
    background: #d35400;
  }
  td.institution:hover .view-btn {
    display: inline-block;
  }

  .modal {
    display: none;
    position: fixed;
    z-index: 1000;
    left: 0; top: 0;
    width: 100%; height: 100%;
    background: rgba(0,0,0,0.6);
    justify-content: center;
    align-items: center;
  }
  .modal-content {
    background: #fff;
    padding: 25px 30px;
    border-radius: 12px;
    width: 450px;
    max-width: 90%;
    text-align: left;
    position: relative;
    animation: fadeIn 0.3s ease-in-out;
    box-shadow: 0 12px 24px rgba(0,0,0,0.2);
  }
  .modal-content h2 {
    margin-top: 0;
    color: #2c3e50;
  }
  .close {
    position: absolute;
    right: 15px;
    top: 10px;
    font-size: 22px;
    cursor: pointer;
    color: #333;
  }
  .close:hover {
    color: red;
  }

  @keyframes fadeIn {
    from {opacity: 0; transform: scale(0.9);}
    to {opacity: 1; transform: scale(1);}
  }
</style>

</head>
